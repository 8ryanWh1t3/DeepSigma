# =============================================================================
# COHERENCE OPS — FORMAL ONTOLOGY
# Extracted from: Whimsical "Everything" Map + 284 LinkedIn Posts + Repo Plan
# Deep Sigma / Coherence Ops Framework
# Generated: 2025-02-14
# =============================================================================

# -----------------------------------------------------------------------------
# ONTOLOGY DECLARATION
# -----------------------------------------------------------------------------

<https://deepsigma.io/ontology/coherence-ops>
    a owl:Ontology ;
    rdfs:label "Coherence Ops Framework Ontology"@en ;
    dcterms:title "Deep Sigma — Coherence Operations Formal Ontology"@en ;
    dcterms:description """
        Formal representation of the Coherence Ops framework covering:
        brand hierarchy, architectural layers, operational modes, instruments,
        four-pillar model, PRIME governance gate, and integration adapters.
        Derived from 284 LinkedIn posts, the Whimsical concept map, and the
        sigma-overwatch repository implementation plan (P0–P5).
    """@en ;
    dcterms:creator "Deep Sigma" ;
    dcterms:created "2025-02-14"^^xsd:date ;
    owl:versionInfo "0.1.0" ;
    rdfs:seeAlso <https://github.com/deep-sigma/sigma-overwatch> .


# =============================================================================
# CLASSES — FRAMEWORK TAXONOMY
# =============================================================================

# --- Top-Level Abstract Classes -----------------------------------------------

coh:FrameworkConcept
    a owl:Class ;
    rdfs:label "Framework Concept"@en ;
    rdfs:comment "Any named concept in the Coherence Ops universe." .

coh:Layer
    a owl:Class ;
    rdfs:subClassOf coh:FrameworkConcept ;
    rdfs:label "Architectural Layer"@en ;
    rdfs:comment "A structural layer in the Coherence Ops stack." .

coh:Pillar
    a owl:Class ;
    rdfs:subClassOf coh:FrameworkConcept ;
    rdfs:label "Pillar"@en ;
    rdfs:comment "One of the four foundational pillars of Coherence Ops." .

coh:OperationalMode
    a owl:Class ;
    rdfs:subClassOf coh:FrameworkConcept ;
    rdfs:label "Operational Mode"@en ;
    rdfs:comment "A domain-specific configuration bundle (policy pack + PRIME thresholds + IRIS templates)." .

coh:Instrument
    a owl:Class ;
    rdfs:subClassOf coh:FrameworkConcept ;
    rdfs:label "Instrument"@en ;
    rdfs:comment "A coherence measurement or assessment tool." .

coh:Invariant
    a owl:Class ;
    rdfs:subClassOf coh:FrameworkConcept ;
    rdfs:label "Invariant"@en ;
    rdfs:comment "A truth/reasoning/memory invariant enforced by the PRIME gate." .

coh:Adapter
    a owl:Class ;
    rdfs:subClassOf coh:FrameworkConcept ;
    rdfs:label "Adapter"@en ;
    rdfs:comment "An integration adapter connecting Coherence Ops to external systems." .

coh:Metaphor
    a owl:Class ;
    rdfs:subClassOf coh:FrameworkConcept ;
    rdfs:label "Metaphor"@en ;
    rdfs:comment "An explanatory model or analogy used to communicate framework concepts." .

coh:Pattern
    a owl:Class ;
    rdfs:subClassOf coh:FrameworkConcept ;
    rdfs:label "Pattern"@en ;
    rdfs:comment "A recurring architectural or operational pattern in the framework." .


# --- Governance Sub-Classes ---------------------------------------------------

coh:GovernanceAction
    a owl:Class ;
    rdfs:label "Governance Action"@en ;
    rdfs:comment "An action output by the PRIME Threshold Gate." ;
    owl:oneOf ( coh:Approve coh:Defer coh:Escalate ) .

coh:DriftType
    a owl:Class ;
    rdfs:label "Drift Type"@en ;
    rdfs:comment "A category of coherence drift detected by the Drift Scan pillar." .

coh:ThreatLevel
    a owl:Class ;
    rdfs:label "Threat Level"@en ;
    rdfs:comment "CTI threat classification." ;
    owl:oneOf ( coh:ThreatGreen coh:ThreatYellow coh:ThreatRed ) .

coh:DegradeTier
    a owl:Class ;
    rdfs:label "Degrade Tier"@en ;
    rdfs:comment "A tier in the degrade ladder escalation model." .


# =============================================================================
# PROPERTIES
# =============================================================================

# --- Structural Relations -----------------------------------------------------

coh:hasLayer
    a owl:ObjectProperty ;
    rdfs:label "has layer"@en ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range coh:Layer .

coh:hasPillar
    a owl:ObjectProperty ;
    rdfs:label "has pillar"@en ;
    rdfs:domain coh:Layer ;
    rdfs:range coh:Pillar .

coh:hasMode
    a owl:ObjectProperty ;
    rdfs:label "has operational mode"@en ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range coh:OperationalMode .

coh:hasInstrument
    a owl:ObjectProperty ;
    rdfs:label "has instrument"@en ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range coh:Instrument .

coh:enforcesInvariant
    a owl:ObjectProperty ;
    rdfs:label "enforces invariant"@en ;
    rdfs:domain coh:Layer ;
    rdfs:range coh:Invariant .

coh:queriesThrough
    a owl:ObjectProperty ;
    rdfs:label "queries through"@en ;
    rdfs:comment "Relationship: an operator interface queries a pillar or layer." ;
    rdfs:domain coh:Layer ;
    rdfs:range coh:Pillar .

coh:triggersInstrument
    a owl:ObjectProperty ;
    rdfs:label "triggers instrument"@en ;
    rdfs:domain coh:Pillar ;
    rdfs:range coh:Instrument .

coh:escalatesTo
    a owl:ObjectProperty ;
    rdfs:label "escalates to"@en ;
    rdfs:domain coh:DegradeTier ;
    rdfs:range coh:DegradeTier .

coh:dependsOn
    a owl:ObjectProperty ;
    rdfs:label "depends on"@en ;
    rdfs:comment "Implementation dependency between concepts." ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range coh:FrameworkConcept .

coh:analogTo
    a owl:ObjectProperty ;
    rdfs:label "analogous to"@en ;
    rdfs:domain coh:Metaphor ;
    rdfs:range coh:FrameworkConcept .

# --- Data Properties ----------------------------------------------------------

coh:postCount
    a owl:DatatypeProperty ;
    rdfs:label "LinkedIn post count"@en ;
    rdfs:comment "Number of posts (out of 284) that reference this concept." ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range xsd:integer .

coh:postPercentage
    a owl:DatatypeProperty ;
    rdfs:label "content coverage percentage"@en ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range xsd:decimal .

coh:repoStatus
    a owl:DatatypeProperty ;
    rdfs:label "repository implementation status"@en ;
    rdfs:comment "One of: implemented, partial, planned, content-only." ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range xsd:string .

coh:implementationPhase
    a owl:DatatypeProperty ;
    rdfs:label "implementation phase"@en ;
    rdfs:comment "The repo plan phase: pre, P0, P1, P2, P3, P4, P5." ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range xsd:string .

coh:gapType
    a owl:DatatypeProperty ;
    rdfs:label "gap type"@en ;
    rdfs:comment "CONTENT>CODE, CODE>CONTENT, PARTIAL_GAP, ALIGNED, or LOW_SIGNAL." ;
    rdfs:domain coh:FrameworkConcept ;
    rdfs:range xsd:string .


