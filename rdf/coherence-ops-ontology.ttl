# =============================================================================
# INDIVIDUALS — ARCHITECTURAL LAYERS
# =============================================================================

# --- PRIME Layer (P1) ---------------------------------------------------------

coh:PRIMELayer
    a coh:Layer ;
    rdfs:label "PRIME Threshold Gate"@en ;
    rdfs:comment """
        Governance layer converting LLM probability gradients into
        decision-grade actions (APPROVE/DEFER/ESCALATE) using
        Truth-Reasoning-Memory invariants.
    """@en ;
    coh:postCount 37 ;
    coh:postPercentage 13.0 ;
    coh:repoStatus "implemented" ;
    coh:implementationPhase "P1" ;
    coh:gapType "ALIGNED" ;
    coh:enforcesInvariant coh:TruthInvariant, coh:ReasoningInvariant, coh:MemoryInvariant ;
    coh:dependsOn coh:DLR, coh:RS, coh:DS, coh:MG .

coh:Approve   a coh:GovernanceAction ; rdfs:label "APPROVE" .
coh:Defer     a coh:GovernanceAction ; rdfs:label "DEFER" .
coh:Escalate  a coh:GovernanceAction ; rdfs:label "ESCALATE" .

# --- IRIS Layer (P2) ----------------------------------------------------------

coh:IRISLayer
    a coh:Layer ;
    rdfs:label "IRIS Interface"@en ;
    rdfs:comment """
        Operator-facing query resolution engine. Handles WHY, WHAT_CHANGED,
        WHAT_DRIFTED, RECALL, STATUS queries with sub-60-second response target.
        Terminal access to the PRIME layer.
    """@en ;
    coh:postCount 15 ;
    coh:postPercentage 5.3 ;
    coh:repoStatus "planned" ;
    coh:implementationPhase "P2" ;
    coh:gapType "CONTENT>CODE" ;
    coh:queriesThrough coh:DLR, coh:DS, coh:MG ;
    coh:dependsOn coh:PRIMELayer .


